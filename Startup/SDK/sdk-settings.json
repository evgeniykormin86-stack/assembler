{
  "python": {
    "name": "assembler-python-sdk",
    "container_name": "assembler_python_sdk",
    "ports": {"8000/tcp": 8000},
    "environment": {
      "FLASK_APP": "Server",
      "FLASK_ENV": "development"
    },
    "volumes": {
      "D:/MyProjects/assembler/Backend": {
        "bind": "/app",
        "mode": "rw"
      }
    },
    "network": "assembler",
    "version": "dev"
  }
}